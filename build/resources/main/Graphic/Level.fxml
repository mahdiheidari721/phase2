<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="398.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Graphic.Level">
   <children>
      <ImageView fitHeight="500.0" fitWidth="1000.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@LevelBackground.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="75.0" fitWidth="118.0" layoutY="426.0" pickOnBounds="true" preserveRatio="true" onMousePressed="#backPressed">
         <image>
            <Image url="@back.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="153.0" fitWidth="337.0" layoutX="82.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Levelicon.png" />
         </image>
      </ImageView>
      <Button fx:id="button1" layoutX="223.0" layoutY="177.0" mnemonicParsing="false" onAction="#level1" style="-fx-background-color: #ff0000;" text="Level 1">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="button2" layoutX="223.0" layoutY="216.0" mnemonicParsing="false" onAction="#level2" style="-fx-background-color: #0090ff;" text="Level 2">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="button3" layoutX="223.0" layoutY="256.0" mnemonicParsing="false" onAction="#level3" style="-fx-background-color: #00f4eb;" text="Level 3">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="button4" layoutX="223.0" layoutY="295.0" mnemonicParsing="false" onAction="#level4" style="-fx-background-color: #c9dd33;" text="Level 4">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
      <Button fx:id="button5" layoutX="223.0" layoutY="335.0" mnemonicParsing="false" onAction="#level5" style="-fx-background-color: #ffc300;" text="Level 5">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
